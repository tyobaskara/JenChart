{
  "presets": ["env", "react"],
  "plugins": [
    "transform-class-properties",
    "babel-plugin-transform-object-rest-spread",
    [
      "file-loader",
      {
        "name": "[name].[ext]",
        "extensions": ["png", "jpg", "jpeg", "gif", "svg"],
        "publicPath": "public",
        "outputPath": "public",
      }
    ]
  ]
}
